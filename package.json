        "crypto-browserify": "^3.12.0",
        "electron-squirrel-startup": "^1.0.0",
        "electron-window-state": "^5.0.3",
        "event-kit": "^2.5.3",
        "json5": "^2.2.3",
        "path-browserify": "^1.0.1",
        "preact": "^10.17.0",
        "signals": "^1.0.0",
        "stats.js": "^0.17.0",
        "three": "^0.155.0",
        "underscore-plus": "^1.7.0"
    },
    "packageManager": "yarn@3.1.1"
}
